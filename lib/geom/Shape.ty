module Trinity
    module Geometry
        class Shape
            def getBounds
                Rectangle.new(0, 0, 0, 0)

            def contains(x, y)
                false

            def containsPoint(point)
                contains(point.getX(), point.getY())

            def intersects(x, y, w, h)
                false

            def intersectsRect(rect)
                intersects(rect.getX(), rect.getY(), rect.getWidth(), rect.getHeight())
